
<h3>{{viewTitle}}</h3>
<p>Tous nos articles sont ici!</p>

  
  <div class="row">

      <div class="col s3">
       
         {{#each list}}
            <li><a href="/{{this.id}}">{{this.name}}</a></li>
        {{/each}}
   
      </div>
    <div class="col s9">{{#each articles}}
      <div class="card large">
        <div class="card-image">
          <img src="{{this.type}}">
          <span class="card-title">{{this.title}}</span>
        </div>
        <div class="card-content">
          <p>{{this.name}}</p>
        </div>
         <div class="card-content">
          <p>{{this.user_firstname}} {{user_lastname}}</p>
        </div>
        <div class="card-content">
          <p>{{this.content}}</p>
        </div>
        
         <div class="card-content">
          <p>{{this.date_creation}}</p>
        </div>
        <div class="card-action">
          <a href="/{{this.id}}"> Voir l'article</a>
              
        </div>{{/each}}
      </div>
    </div>
  </div>
   
